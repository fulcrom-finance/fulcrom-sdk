{
  "name": "@fulcrom-finance/fulcrom-sdk",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "typechain": "typechain --out-dir src/types/contracts --target ethers-v5 'src/abis/*.json'",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "ts-check": "tsc --noEmit",
    "test": "jest",
    "dev": "tsup --watch",
    "build": "rm -rf dist && tsup",
    "release": "pnpm publish --access public",
    "release-local": "yalc publish"
  },
  "keywords": [
    "fulcrom"
  ],
  "homepage": "https://github.com/fulcrom-finance/fulcrom-sdk#readme",
  "bugs": "https://github.com/fulcrom-finance/fulcrom-sdk/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fulcrom-finance/fulcrom-sdk.git"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "sourcemap": true,
    "clean": true,
    "dts": true
  },
  "exports": {
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "devDependencies": {
    "@fulcrom-finance/eslint-config": "workspace:*",
    "@fulcrom-finance/typescript-config": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "5.5.4",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@ethersproject/solidity": "^5.7.0",
    "@ethersproject/units": "^5.7.0",
    "@pythnetwork/pyth-evm-js": "^1.16.1",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/node-fetch": "^2.6.12",
    "ethers": "^5.7.1",
    "node-fetch": "^2.7.0",
    "typechain": "^8.1.0"
  }
}
